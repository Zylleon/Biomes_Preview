<!--====================	Small Birds		=====================-->
  <ThingDef ParentName="BigBirdThingBase">
    <defName>BiomesIslands_KiwiBird</defName>
    <label>kiwi</label>
    <description>Tiny round bird. It's lack of flight makes it helpless against even small predators. No relation to the fruit.</description>
    <statBases>
      <MoveSpeed>3.1</MoveSpeed>
      <MarketValue>80</MarketValue>
      <ComfyTemperatureMin>-8</ComfyTemperatureMin>
	  <LeatherAmount>5</LeatherAmount>
    </statBases>
    <race>
      <herdAnimal>false</herdAnimal>
      <baseBodySize>0.15</baseBodySize>
		<baseHealthScale>0.5</baseHealthScale>
		<baseHungerRate>0.4</baseHungerRate>
		<foodType>VegetarianRoughAnimal</foodType>
      <wildness>0.45</wildness>
      <trainability>Simple</trainability>
      <lifeExpectancy>15</lifeExpectancy>
      <manhunterOnTameFailChance>0.01</manhunterOnTameFailChance>
      <manhunterOnDamageChance>0</manhunterOnDamageChance>
	  <nuzzleMtbHours>48</nuzzleMtbHours>
	  <nameOnTameChance>0.5</nameOnTameChance>
	  <petness>0.1</petness>
    </race>
    <comps>
      <li Class="CompProperties_EggLayer">
        <eggFertilizedDef>BiomesIslands_EggKiwiFertilized</eggFertilizedDef>
        <eggFertilizationCountMax>2</eggFertilizationCountMax>
        <eggLayIntervalDays>9</eggLayIntervalDays>
        <eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
        <eggCountRange>
          <min>1</min>
          <max>2</max>
        </eggCountRange>
      </li>
    </comps>
    <tools>
      <li>
        <label>claws</label>
        <capacities>
          <li>Scratch</li>
        </capacities>
        <power>3</power>
        <cooldownTime>1.5</cooldownTime>
        <linkedBodyPartsGroup>Feet</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>beak</label>
        <capacities>
          <li>Bite</li>
        </capacities>
        <power>4</power>
        <cooldownTime>1.8</cooldownTime>
        <linkedBodyPartsGroup>Beak</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>14</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>2</power>
        <cooldownTime>1.8</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <tradeTags>
      <li>AnimalUncommon</li>
      <li>AnimalPet</li>
    </tradeTags>
  </ThingDef>

